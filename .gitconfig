[alias]
    bb = !"$HOME/.zsh/autoload/git-bb.sh"
[core]
	editor = nvim
[diff]
	colorMoved = default
	renameLimit = 0

	submodule = log
    tool = nvimdiff
[difftool]
    prompt = false
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[user]
	name = yesoer
	email = 71628012+yesoer@users.noreply.github.com
